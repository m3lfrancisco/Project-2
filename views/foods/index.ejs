<%- include('../partials/header') %>

<table>
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        <% foods.forEach(function(f) { %>
            <tr>
                <td><%= f.name %></td>
                <td><a href="/foods/<%= f._id %>">DETAILS</a> </td>
            </tr>
        <% }); %>
    </tbody>
</table>

<%- include('../partials/footer') %>